# Copy this file to `.env` and fill with your real keys.
# Never commit real keys.

# ------------------------------------------------------------
# Runtime keys (supported in code)
# ------------------------------------------------------------
OPENROUTER_KEY=sk-or-v1-your-openrouter-key
OPENAI_API_KEY=sk-proj-your-openai-key
ANTHROPIC_API_KEY=sk-ant-api03-your-anthropic-key
XAI_API_KEY=xai-your-xai-key

# Provider to use by default: openrouter | openai | anthropic | xai
AI_PROVIDER=openrouter

# Default model id (optional, can be overridden in app settings)
DEFAULT_MODEL=anthropic/claude-sonnet-4-5

# Max output tokens (optional)
MAX_TOKENS=2048
